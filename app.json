{
  "expo": {
    "name": "pick-a-card",
    "slug": "pick-a-card",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon-custom.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/favicon-custom.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF2E63"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon-custom.png",
        "backgroundColor": "#FF2E63"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon-custom.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
